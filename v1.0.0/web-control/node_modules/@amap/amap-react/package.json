{
  "name": "@amap/amap-react",
  "version": "0.1.5",
  "main": "lib/index.js",
  "module": "es/index.js",
  "umd:main": "dist/amap-react.umd.js",
  "typings": "lib/types/index.d.ts",
  "files": [
    "dist/",
    "lib/",
    "es/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiuJi-Jim/amap-react.git"
  },
  "homepage": "https://jimnox.gitee.io/amap-react/",
  "bugs": {
    "url": "https://github.com/LiuJi-Jim/amap-react/issues"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@docusaurus/core": "2.0.0-alpha.66",
    "@docusaurus/module-type-aliases": "^2.0.0-alpha.51",
    "@docusaurus/preset-classic": "2.0.0-alpha.66",
    "@mdx-js/react": "^1.5.8",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "antd": "^4.7.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-define": "^2.0.0",
    "clsx": "^1.1.1",
    "eslint": "^7.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.1.2",
    "glob": "^7.1.6",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.1",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "docs:start": "cd docs && docusaurus start --port=8080",
    "docs:build": "cd docs && docusaurus build",
    "dev": "npm run docs:start",
    "serve": "npm run docs:serve",
    "docs": "npm run build:gen-es && rm -rf docs/build && npm run docs:build",
    "build:gen-es": "node build/gen-es.js && cp build/es-list.json docs/components/",
    "build:clean": "rimraf ./dist ./lib ./es",
    "build:rollup": "rollup -c build/rollup.config.js",
    "build:copy-types": "cp -r src/types dist/ && cp -r src/types lib/ && cp -r src/types es/",
    "build": "npm run build:clean && npm run build:gen-es && npm run build:rollup && npm run build:copy-types",
    "type-check": "tsc --noEmit",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "cloc": "cloc src/ docs/docs/ docs/components/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
