{
    "name": "inferencejs",
    "version": "1.0.21",
    "description": "",
    "files": [
        "dist"
    ],
    "main": "./dist/inference.js",
    "module": "./dist/inference.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/inference.es.js",
            "require": "./dist/inference.js"
        }
    },
    "scripts": {
        "test": "jest --coverage --verbose",
        "test:watch": "jest --watch",
        "build-ts": "tsc",
        "dev": "vite",
        "clean": "rm -rf dist public",
        "build": "mkdir public; vite build && cp -r dist/* public || true",
        "build:worker": "mkdir public; vite build --config worker.vite.config.js && mv dist/* public || true",
        "build:demo": "mkdir public; npm run build:worker && vite build --config demo.vite.config.js --base inferencejs && mv dist/* public || true",
        "preview": "vite preview",
        "prepublish": "npm run build",
        "deploy-pages": "npm run clean && npm run build:demo && gh-pages -d public"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/jest": "^29.5.11",
        "@types/node": "^20.11.10",
        "autoprefixer": "^10.4.18",
        "gh-pages": "6.1.1",
        "jest": "^29.7.0",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom-worker": "^0.3.0",
        "postcss": "^8.4.37",
        "tailwindcss": "^3.4.1",
        "ts-jest": "^29.2.5",
        "typescript": "^5.3.3",
        "vite": "^5.3.3",
        "vite-plugin-dts": "^3.7.2"
    },
    "dependencies": {
        "@babel/preset-env": "^7.25.8",
        "@mediapipe/tasks-vision": "^0.10.12",
        "@tensorflow-models/face-detection": "^1.0.2",
        "@tensorflow/tfjs": "^4.21.0",
        "babel-jest": "^29.7.0",
        "simple-fingerprint": "^1.1.1"
    }
}